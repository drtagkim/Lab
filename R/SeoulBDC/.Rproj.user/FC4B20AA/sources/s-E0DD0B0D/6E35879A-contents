% Generated by roxygen2 (4.1.1): do not edit by hand
% Please edit documentation in R/stegasaur.R
\name{encode}
\alias{encode}
\title{Encode information into different forms}
\usage{
encode(content, file_in, file_out)
}
\arguments{
\item{content}{Content to encode: any R object is OK (see
\code{\link{lsb_encode}}).}

\item{file_in}{Source filename}

\item{file_out}{Destination filename (can be the same as the
source filename for a destructive update)}
}
\description{
Encode information into different forms
}
\examples{
file_in <- system.file("man_with_cats.jpg")
x <- runif(10)
encode(x, file_in, 'plot.png')
# see also:
decode(file_out)
}

